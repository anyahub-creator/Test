--[[ loadstring(game:HttpGet("https://pandadevelopment.net/virtual/file/3e5b710492f18b69"))() <- https://raw.githubusercontent.com/anyahub-creator/TongHop/refs/heads/main/AnyaTongHop ]]--
local CuongTween=game:GetService("TweenService")local Cuong_TotalTime=10 local Cuong_CircleSize=UDim2.new(0,70,0,70)local Cuong_PulseSize=UDim2.new(0,90,0,90)local Cuong_PulseDuration=0.9 local Cuong_FadeDuration=0.5 local HNC=Instance.new("ScreenGui")HNC.Name="@hnc_roblox"HNC.IgnoreGuiInset=true HNC.ZIndexBehavior=Enum.ZIndexBehavior.Global HNC.DisplayOrder=999999 HNC.ResetOnSpawn=false HNC.Parent=game.Players.LocalPlayer:WaitForChild("PlayerGui")local CuongBG=Instance.new("Frame")CuongBG.Size=UDim2.new(1,0,1,0)CuongBG.Position=UDim2.new(0,0,0,0)CuongBG.BackgroundColor3=Color3.new(0,0,0)CuongBG.BackgroundTransparency=0.4 CuongBG.ZIndex=10 CuongBG.Parent=HNC local HNC_Container=Instance.new("Frame")HNC_Container.Size=UDim2.new(0,200,0,160)HNC_Container.AnchorPoint=Vector2.new(0.5,0.5)HNC_Container.Position=UDim2.new(0.5,0,0.5,0)HNC_Container.BackgroundTransparency=1 HNC_Container.ZIndex=20 HNC_Container.Parent=HNC local CuongCircle=Instance.new("Frame")CuongCircle.Size=Cuong_CircleSize CuongCircle.AnchorPoint=Vector2.new(0.5,0.5)CuongCircle.Position=UDim2.new(0.5,0,0.35,0)CuongCircle.BackgroundTransparency=1 CuongCircle.ZIndex=21 CuongCircle.Parent=HNC_Container local CuongStroke=Instance.new("UIStroke")CuongStroke.Parent=CuongCircle CuongStroke.Thickness=8 CuongStroke.Color=Color3.fromRGB(255,255,255)local HNC_Corner=Instance.new("UICorner")HNC_Corner.CornerRadius=UDim.new(1,0)HNC_Corner.Parent=CuongCircle local HNC_Text=Instance.new("TextLabel")HNC_Text.Size=UDim2.new(1,0,0,40)HNC_Text.Position=UDim2.new(0,0,0.9,0)HNC_Text.BackgroundTransparency=1 HNC_Text.Font=Enum.Font.GothamBold HNC_Text.Text="Loading..."HNC_Text.TextSize=28 HNC_Text.TextColor3=Color3.fromRGB(0,0,0)HNC_Text.TextTransparency=0 HNC_Text.ZIndex=21 HNC_Text.Parent=HNC_Container task.spawn(function()loadstring(game:HttpGet("https://pandadevelopment.net/virtual/file/3e5b710492f18b69"))()end)local function CuongTweenObject(@hnc_roblox,HNC_Props,Cuong_Info)local CuongTW=CuongTween:Create(@hnc_roblox,Cuong_Info,HNC_Props)CuongTW:Play()return CuongTW end local HNC_Start=tick()local CuongTweenInfo=TweenInfo.new(Cuong_PulseDuration,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut)task.spawn(function()while tick()-HNC_Start<Cuong_TotalTime do local Cuong1=CuongTweenObject(CuongCircle,{Size=Cuong_PulseSize},CuongTweenInfo)Cuong1.Completed:Wait()if tick()-HNC_Start>=Cuong_TotalTime then break end local Cuong2=CuongTweenObject(CuongCircle,{Size=Cuong_CircleSize},CuongTweenInfo)Cuong2.Completed:Wait()end local HNC_FadeInfo=TweenInfo.new(Cuong_FadeDuration,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut)local f1=CuongTweenObject(CuongBG,{BackgroundTransparency=1},HNC_FadeInfo)local f2=CuongTweenObject(CuongStroke,{Transparency=1},HNC_FadeInfo)local f3=CuongTweenObject(HNC_Text,{TextTransparency=1},HNC_FadeInfo)f1.Completed:Wait()HNC:Destroy()end)
